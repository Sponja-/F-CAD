class Node {
	function constructor(val) {
		this.val = val;
		this.next = null;
	}
}

class LinkedList {
	function constructor(...numbers) {
		this.head = null;
		this.tail = null;
		for(n in numbers)
			this.append(n);
	}

	function append(n) {
		if(this.head == null) {
			this.head = new Node(n);
			this.tail = this.head;
		}
		else {
			this.tail.next = new Node(n);
			this.tail = this.tail.next;
		}
	}

	function str() {
		s = "" : this.head.val;
		node = this.head.next;
		while(node.next != null) {
			s = s : ", " : node.val;
			node = node.next;
		}
		s = s : ", " : node.val;
		return s;
	}

	function copy() {
		c = new LinkedList();
		node = this.head;
		while(node != null) {
			c.append(node.val);
			node = node.next;
		}
		return c;
	}

	function __add__(other) {
		result = c.copy();
		result.tail.next = other.copy().head;
		return result;
	}
}

l1 = new LinkedList(1, 2, 3);
l2 = new LinkedList(4, 5, 6);
print(l1.str());
l3 = l1.copy();
print(l3.str());
l4 = l1 + l2;
print(l4.str());